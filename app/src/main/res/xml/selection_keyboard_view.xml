<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="20%p"
    android:keyHeight="10%p" >

    <Row>
        <Key android:codes="5"
            android:keyLabel="@string/cut"/>
        <Key android:codes="6"
            android:keyLabel="@string/copy"/>
        <Key android:keyOutputText=""
            android:keyLabel=" "/>
        <Key android:codes="2"
            android:keyLabel="@string/dpadUp"
            android:isRepeatable="true" />
        <Key android:keyOutputText=""
            android:keyLabel=" "/>
    </Row>
    <Row>
        <Key android:codes="7"
            android:keyLabel="@string/paste"/>
        <Key android:codes="10"
            android:keyLabel="@string/delete"/>
        <Key android:codes="0"
            android:keyLabel="@string/dpadLeft"
            android:isRepeatable="true" />
        <Key android:codes="9"
            android:keyLabel="@string/dpadCenter"
            android:isSticky="true"/>
        <Key android:codes="1"
            android:keyLabel="@string/dpadRight"
            android:isRepeatable="true"/>
    </Row>

    <Row>
        <Key android:codes="4"
            android:keyLabel="@string/MainKeyboard"/>
        <Key android:codes="8"
            android:keyLabel="@string/selectAll"/>
        <Key android:keyOutputText=""
            android:keyLabel=" "/>
        <Key android:codes="3"
            android:keyLabel="@string/dpadDown"
            android:isRepeatable="true" />
        <Key android:keyOutputText=""
            android:keyLabel=" "/>
    </Row>
</Keyboard>